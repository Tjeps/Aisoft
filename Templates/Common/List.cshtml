@using RazorEngine.Templating
@using Mandeeps.LSF.Data.Entities
@inherits TemplateBase<dynamic>

@if (Model != null && Model.Count > 0)
{
    <ul class="ms-nav-pills ms-nav-stacked widget_sidebar">
        @foreach (var item in Model)
        { 
            <li class="@(item.IsSelected ? "ms-active" : null)">
				<span class="arrow-icon"><em class="fa fa-chevron-right"></em></span><a rel="ajaxload" href="@item.PermLink"> @item.Name</a>
                @Include("Common/List", @item.Children)
            </li>
        }
    </ul>
}
    